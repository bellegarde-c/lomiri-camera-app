[Desktop Entry]
Type=Application
Name=Barcode Reader
Comment=Barcode Reader application
GenericName=Barcode Reader
Keywords=QR;Code;Reader
Exec=@DESKTOP_EXEC@ --mode=barcode-reader %u
Terminal=false
Icon=@READER_ICON@
X-Lomiri-Touch=true
